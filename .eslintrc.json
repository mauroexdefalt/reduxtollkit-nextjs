{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:react/recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [
            "error",
            {
                "extensions": [".js" , ".jsx"]
            }
        ],
        "global-require":"off",
        "import/prefer-default-export":"off",
        "no-unused-expressions":["error",{"allowTaggedTemplates": true}],
        "react/prop-types": "off",
        "react/react-in-jsx-scope":"off"
        
    }
}
